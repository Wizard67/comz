{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "comz": ["index.ts"],
    },
    "target": "es6",
    "lib": ["dom", "esnext"],
    "declaration": true,
    "declarationDir": "types",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true
  },
  "include": [
    "defines/vue.d.ts",
    "defines/createBlock.d.ts",
    "index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}